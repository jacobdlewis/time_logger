%div.row
  %div.small-12.columns
    %h1.page-title Time Entries for #{current_user.name}
    %hr
    %table.time-entries-table
      %tbody
        -@time_entries.each do |entry|
          %tr
            %td
              = link_to "#{entry.date.strftime("%A, %B %d, %Y")} - #{entry.hours_worked} hours", user_time_entry_path(current_user, entry.id)
    = will_paginate @time_entries, renderer: BootstrapPagination::Rails
